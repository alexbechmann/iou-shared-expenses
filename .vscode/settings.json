{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/*.js.map": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.d.ts": {
            "when": "$(basename).ts"
        },
        "core/lib/**": true,
        "parse-server/lib/**": true,
        "parse-server/cloud/**": true,
    },
    "editor.tabSize": 2
}
